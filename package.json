{
  "name": "image-recognition",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "compose": "docker-compose -f ./docker-compose.yml",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node ./src/main.ts",
    "start:nest": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate:seed:demo": "npx yarn db:migrate:seed && npx yarn db:seed:demo",
    "db:migrate:seed": "npx yarn db:migrate && npx yarn db:seed",
    "db:migrate:seed:ecs": "npx yarn db:migrate:seed",
    "db:drop": "yarn typeorm schema:drop",
    "db:drop:test": "TEST_DB=true yarn typeorm schema:drop",
    "db:migrate:nobuild": "yarn typeorm migration:run",
    "db:migrate": "yarn db:migrate:nobuild",
    "db:migrate:create": "yarn typeorm migration:generate -n",
    "db:migrate:revert": "yarn typeorm migration:revert",
    "db:migrate:test": "TEST_DB=true yarn db:migrate",
    "db:reset": "yarn db:reset:server",
    "db:reset:server": "yarn db:drop && yarn db:migrate && yarn db:seed",
    "db:reset:seed": "yarn db:reset && yarn db:seed:demo",
    "db:reset:seed:ecs": "npx yarn db:reset:seed",
    "db:reset:seed:server": "yarn db:reset:server && yarn db:seed && yarn db:seed:demo",
    "db:reset:test": "yarn db:drop:test && yarn db:migrate:test && yarn db:seed:test",
    "db:seed:create": "yarn typeorm migration:create -c seed -n",
    "db:seed": "yarn typeorm migration:run -c seed",
    "db:seed:test": "TEST_DB=true yarn typeorm migration:run -c seed",
    "db:seed:demo": "yarn typeorm migration:run -c demo-seed",
    "db:seed:demo:create": "yarn typeorm migration:create -c demo-seed -n",
    "db:seed:revert": "yarn typeorm migration:revert -c seed",
    "db:sync": "yarn typeorm schema:sync",
    "typeorm": "ts-node -r module-alias/register ./node_modules/typeorm/cli.js  --config ./src/ormconfig.ts"
  },
  "dependencies": {
    "@google-cloud/vision": "^2.4.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/gm": "^1.18.10",
    "fs-jetpack": "^4.3.1",
    "gm": "^1.23.1",
    "imagemagick": "^0.1.3",
    "module-alias": "^2.2.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/imagemagick": "^0.0.31",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "_moduleAliases": {
    "src": "src"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
